{
    "name": "photon",
    "version": "0.2.0-beta",
    "main": "./dist/main.js",
    "license": "MIT",
    "private": true,
    "homepage": "./",
    "repository": "https://github.com/TeamDock/photon",
    "description": "A simple terminal using Electron",
    "author": {
        "name": "Dock",
        "email": "contato.pedrinfx@gmail.com"
    },
    "scripts": {
        "dev": "concurrently -k \"cross-env BROWSER=none npm run react:start\" \"npm run electron:start\"",
        "react:build": "cross-env NODE_ENV=production webpack --config ./webpack/react.webpack.js --mode production",
        "electron:build": "cross-env NODE_ENV=production webpack --config ./webpack/electron.webpack.js --mode production",
        "cli:build": "cross-env NODE_ENV=production webpack --config ./webpack/cli.webpack.js --mode production",
        "react:start": "webpack-dev-server --config ./webpack/react.webpack.js --mode development",
        "electron:start": "wait-on http://localhost:3000 && webpack --config ./webpack/electron.webpack.js --mode development && electron .",
        "build": "npm run electron:build && npm run react:build && npm run cli:build",
        "release": "npm run build && electron-builder --win",
        "rebuild": "electron-rebuild"
    },
    "dependencies": {
        "@electron/remote": "^2.0.1",
        "appdata-path": "^1.0.0",
        "args": "^5.0.1",
        "axios": "^0.24.0",
        "chalk": "^4.1.2",
        "commander": "^8.3.0",
        "electron-is-dev": "^2.0.0",
        "electron-store": "^8.0.1",
        "find-process": "^1.4.7",
        "fs-extra": "^10.0.0",
        "json-schema-typed": "^7.0.3",
        "native-reg": "^1.0.0",
        "node-pty": "0.10.0",
        "open": "^8.4.0",
        "ora": "^5.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1",
        "react-router-dom": "^6.0.2",
        "sass": "^1.43.5",
        "shebang-loader": "^0.0.1",
        "xterm": "^4.15.0",
        "xterm-addon-fit": "^0.5.0",
        "xterm-addon-web-links": "^0.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@types/args": "^5.0.0",
        "@types/electron": "^1.6.10",
        "@types/node": "^16.11.12",
        "@types/ora": "^3.2.0",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "babel-loader": "^8.2.3",
        "concurrently": "^6.4.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "electron": "^16.0.4",
        "electron-builder": "^22.14.5",
        "electron-rebuild": "^3.2.5",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^5.1.1",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "html-webpack-plugin": "^5.5.0",
        "prettier": "^2.5.0",
        "sass-loader": "^12.4.0",
        "style-loader": "^3.3.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.2",
        "wait-on": "^6.0.0",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0"
    }
}
